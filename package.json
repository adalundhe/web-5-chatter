{
  "name": "web5-chatter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "prisma generate && prisma migrate deploy",
    "build:1-next": "cross-env NODE_ENV=production next build",
    "build:2-server": "tsc --project tsconfig.server.json",
    "build": "run-s build:*",
    "dev:wss": "cross-env PORT=3001 tsx watch src/server/api/websocket_dev_server.ts --tsconfig tsconfig.server.json ",
    "dev:next": "next dev",
    "dev": "pnpm migrate-dev && run-p dev:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "dev-nuke": "rm -rf prisma/*.db**",
    "generate": "prisma generate",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "start": "cross-env NODE_ENV=production node dist/server/prodServer.js",
    "studio": "prisma studio",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "test-dev": "cross-env APP_ENV=test start-server-and-test dev 3000 test",
    "test-start": "cross-env APP_ENV=test WS_URL=localhost:3000 start-server-and-test start 3000 test",
    "test": "playwright test",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^5.0.0",
    "@t3-oss/env-nextjs": "^0.3.1",
    "@tanstack/react-query": "^4.29.7",
    "@tbd54566975/dids": "^0.1.9",
    "@tbd54566975/dwn-sdk-js": "^0.2.1",
    "@tbd54566975/web5": "^0.7.11",
    "@tbd54566975/web5-agent": "^0.1.7",
    "@trpc/client": "^10.26.0",
    "@trpc/next": "^10.26.0",
    "@trpc/react-query": "^10.26.0",
    "@trpc/server": "^10.26.0",
    "clsx": "^2.0.0",
    "dexie": "^3.2.4",
    "multiformats": "^12.0.1",
    "next": "^13.4.2",
    "next-auth": "^4.22.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-terminal": "^1.3.1",
    "react-terminal-ui": "^1.0.5",
    "superjson": "1.12.2",
    "uuid": "^9.0.0",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@playwright/test": "^1.36.2",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.16.0",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prisma": "^5.0.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vite": "^4.4.9",
    "vitest": "^0.34.1",
    "zustand": "^4.4.1"
  },
  "prisma": {
    "seed": "tsx prisma/setup.ts"
  },
  "ct3aMetadata": {
    "initVersion": "7.17.0"
  }
}
